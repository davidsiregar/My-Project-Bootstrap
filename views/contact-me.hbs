<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>My Project Add</title>
    <link rel="stylesheet" href="/public/style.css" />
  </head>
  <body style="background-color: gainsboro;">

    {{! start }}
    
    <nav class="navbar navbar-expand-xl navbar-light bg-light">
      <div class="container">
        <a class="navbar-brand ms-5" href="/"><img
            src="/public/asset/logodu.png"
            alt=""
          /></a>

        <div class="collapse navbar-collapse" id="navbarNav">
          <ul class="navbar-nav me-auto mt-5 mt-xxl-3">

            <li class="nav-item">
              <a
                class="nav-link active fw-bold ms-5"
                aria-current="page"
                href="/"
              >Home</a>
            </li>

            <li class="nav-item">
              <a
                class="nav-link active ms-1 fw-normal"
                href="/my-project-add"
              >My Project
              </a>
            </li>
          </ul>
          <a
            class="nav-link active btn btn-dark text-white rounded-pill"
            href="/contact-me"
          >Contact Me </a>
        </div>

      </div>
    </nav>
    {{! end }}

    {{!-- start --}}
    <div class="h1 text-center fw-bold">Get in Touch</div>
    <form>
  <div class="mb-3">
    <label for="input-name" class="form-label fw-bold fs-4">Name</label>
    <input type="text" class="form-control text-center" id="input-name" >
  </div>

  <div class="mb-3">
    <label for="input-email" class="form-label fw-bold fs-4">Email</label>
    <input type="email" class="form-control text-center" id="input-email" >
  </div>

  <div class="mb-3">
    <label for="input-phone" class="form-label fw-bold fs-4">Phone Number</label>
    <input type="number" class="form-control text-center" id="input-phone">
  </div>

<div class="mb-3">
  <label for="input-phone" class="form-label fw-bold fs-4">Subject</label>
  <select class="form-select text-center" aria-label="Default select example">
  <option hidden </option>
  <option value="Let's Talk">Let's Talk</option>
  <option value="Let's Collaboration">Let's Collaboration</option>
  <option value="I'm Hiring">I'm Hiring</option>
  </select>
</div>
         <div class='mb-3'>
          <label for='input-content' class='form-label fw-bold fs-4 text-center'>Content</label>
          <textarea
            class='form-control text-center '
            id='input-content'
            rows='7'
            placeholder='Write here ...'
            name='content'
          ></textarea>
        </div>

  <div class="d-flex justify-content-end">
    <button type="submit" class="btn btn-dark mt-3 w-25 p-2 rounded-pill">Submit</button>
  </div>
</form>
    {{!-- end --}}
